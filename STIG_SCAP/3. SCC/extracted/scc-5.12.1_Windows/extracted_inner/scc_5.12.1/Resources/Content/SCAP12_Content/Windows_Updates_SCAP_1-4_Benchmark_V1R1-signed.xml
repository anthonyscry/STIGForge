  <definitions>
    <definition id="oval:navy.navwar.niwcatlantic.scc.windows.test:def:1" version="1" class="compliance">
      <metadata>
        <title>Check to see if any updates are reported as missing from Windows Update</title>
        <description>The OVAL Scanner deterines if Windows Update should be queried via the Internet, or WSUS, or via offline wsusscn2.cab file</description>
      </metadata>
      <criteria operator="AND">
        <criterion comment="Gather a list of updates not installed, and report failures on them." test_ref="oval:navy.navwar.niwcatlantic.scc.windows.test:tst:1"/>
      </criteria>
    </definition>
	
	<definition id="oval:navy.navwar.niwcatlantic.scc.windows.test:def:2" version="1" class="compliance">
      <metadata>
        <title>Check to see if any updates are reported as missing from Windows Update</title>
        <description>The OVAL Scanner deterines if Windows Update should be queried via the Internet, or WSUS, or via offline wsusscn2.cab file</description>
      </metadata>
      <criteria operator="AND">
        <criterion comment="Gather all installed updates, report failures on any not installed." test_ref="oval:navy.navwar.niwcatlantic.scc.windows.test:tst:2"/>
      </criteria>
    </definition>
  </definitions>
  <tests>
