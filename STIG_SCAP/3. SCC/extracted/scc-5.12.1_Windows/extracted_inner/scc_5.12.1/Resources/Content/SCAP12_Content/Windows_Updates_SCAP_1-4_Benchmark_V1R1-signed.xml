      <object object_ref="oval:navy.navwar.niwcatlantic.scc.windows.test:obj:1"/>
	  <state state_ref="oval:navy.navwar.niwcatlantic.scc.windows.test:ste:1"/>
    </wuaupdatesearcher_test>
	
    <wuaupdatesearcher_test xmlns="http://oval.mitre.org/XMLSchema/oval-definitions-5#windows" id="oval:navy.navwar.niwcatlantic.scc.windows.test:tst:2" version="1" comment="Query Windows update to determine if any OS patches are missing" check_existence="any_exist" check="all">
