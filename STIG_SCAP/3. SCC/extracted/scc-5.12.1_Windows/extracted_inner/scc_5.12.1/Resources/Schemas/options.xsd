        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>

  <xs:simpleType name="rel_filepath">
    <xs:annotation>
      <xs:appinfo>A well formed relative file path.</xs:appinfo>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
