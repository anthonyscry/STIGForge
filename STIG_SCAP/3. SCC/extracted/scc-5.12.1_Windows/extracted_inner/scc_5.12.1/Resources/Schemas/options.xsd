 <xs:simpleType name="anyURI">
 <xs:annotation>
      <xs:appinfo>Any URI.</xs:appinfo>
    </xs:annotation>
        <xs:union>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:length value="0"></xs:length>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:anyURI">
          <xs:pattern value="((http://|https://)|(www.))+(([a-zA-Z0-9\.\-/]+\.[a-zA-Z]{2,4})|([0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}))(:[0-9]{1,5})?(/[a-zA-Z0-9%:/\-_\?\.'~]*)?"  />
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
    </xs:simpleType>


    

  <xs:simpleType name="reviewType">
    <xs:annotation>
      <xs:appinfo>local, remote, multiremote, cisco, or cisco-remote.</xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="(local|remote|multiremote|cisco|cisco-remote|unix-remote|windows-unix-remote)"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="summaryViewerSort">
    <xs:annotation>
      <xs:appinfo>Host, Session, or Stream.</xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="(Host|Session|Stream)"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>



  <xs:simpleType name="xccdfFileAgeAction">
    <xs:annotation>
      <xs:appinfo>Ignore, Warn, or Exclude.</xs:appinfo>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:pattern value="(Ignore|Warn|Exclude)"></xs:pattern>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="itemCreationThreshold">
    <xs:annotation>
      <xs:appinfo>An integer between 0 and 999999.</xs:appinfo>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:length value="0"></xs:length>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:pattern value="[0-9]{1,6}"></xs:pattern>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>

  <xs:simpleType name="hostName">
    <xs:annotation>
      <xs:appinfo>A well formed hostname.</xs:appinfo>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:pattern value="[a-zA-Z0-9]{1}[a-zA-Z0-9\.:\-]{0,254}"></xs:pattern>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:length value="0"></xs:length>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>

  <xs:simpleType name="ip">
    <xs:annotation>
      <xs:appinfo>A well formed public or private ipv4 address.</xs:appinfo>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:pattern value="\w((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)"></xs:pattern>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:length value="0"></xs:length>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>


  <xs:simpleType name="less_than_512_chars">
    <xs:annotation>
      <xs:appinfo>Data less than 512 characters.</xs:appinfo>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:pattern value=".{1,512}"></xs:pattern>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:length value="0"></xs:length>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>

  <xs:simpleType name="hostName_or_ip">
    <xs:annotation>
      <xs:appinfo>a hostname or an ipv4 or ipv6.</xs:appinfo>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
        <xs:restriction base="xs:string">
          <xs:pattern value="(\w((25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.){3}(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?))|([a-zA-Z0-9]{1}[a-zA-Z0-9\.\-]{0,254})|(([0-9a-fA-F]{1,4}:){7,7}[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,7}:|([0-9a-fA-F]{1,4}:){1,6}:[0-9a-fA-F]{1,4}|([0-9a-fA-F]{1,4}:){1,5}(:[0-9a-fA-F]{1,4}){1,2}|([0-9a-fA-F]{1,4}:){1,4}(:[0-9a-fA-F]{1,4}){1,3}|([0-9a-fA-F]{1,4}:){1,3}(:[0-9a-fA-F]{1,4}){1,4}|([0-9a-fA-F]{1,4}:){1,2}(:[0-9a-fA-F]{1,4}){1,5}|[0-9a-fA-F]{1,4}:((:[0-9a-fA-F]{1,4}){1,6})|:((:[0-9a-fA-F]{1,4}){1,7}|:)|fe80:(:[0-9a-fA-F]{0,4}){0,4}%[0-9a-zA-Z]{1,}|::(ffff(:0{1,4}){0,1}:){0,1}((25[0-5]|(2[0-4]|1{0,1}[0-9]){0,1}[0-9])\.){3,3}(25[0-5]|(2[0-4]|1{0,1}[0-9]){0,1}[0-9])|([0-9a-fA-F]{1,4}:){1,4}:((25[0-5]|(2[0-4]|1{0,1}[0-9]){0,1}[0-9])\.){3,3}(25[0-5]|(2[0-4]|1{0,1}[0-9]){0,1}[0-9]))"></xs:pattern>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:length value="0"></xs:length>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>

  <xs:simpleType name="username">
    <xs:annotation>
      <xs:appinfo>A well formed username.</xs:appinfo>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:length value="0"></xs:length>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:pattern value="[a-zA-Z0-9@\._-]{1,254}"></xs:pattern>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>

  <xs:simpleType name="portNumber">
    <xs:annotation>
      <xs:appinfo>A port number between 0 and 65536.</xs:appinfo>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:length value="0"></xs:length>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:pattern value="([0-9]{1,4}|[1-5][0-9]{4}|6[0-4][0-9]{3}|65[0-4][0-9]{2}|655[0-2][0-9]|6553[0-5])"></xs:pattern>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>

  <xs:simpleType name="abs_dirpath">
    <xs:annotation>
      <xs:appinfo>A well formed absolute directory path. Windows Style</xs:appinfo>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:length value="0"></xs:length>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:pattern value="(([a-zA-Z]:(\\|/))|(/)|(\\\\)){1}([a-zA-Z0-9\s\._()-@$]+(\\|/)?)*"></xs:pattern>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>
  
  <xs:simpleType name="unix_dirpath">
    <xs:annotation>
      <xs:appinfo>A well formed absolute directory path. UNIX Style</xs:appinfo>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:length value="0"></xs:length>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:pattern value="/+([a-zA-Z0-9\t \._()@$-]+/*)*"></xs:pattern>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>

  <xs:simpleType name="path">
    <xs:annotation>
      <xs:appinfo>A well formed path.</xs:appinfo>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:length value="0"></xs:length>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:pattern value="((([a-zA-Z]:(\\|/))|(/)|(\\\\)){1}([a-zA-Z0-9\s\._()-]+(\\|/)?)*)|(/.*)|([a-zA-Z0-9\s\._()-]+(\\|/)?)*"></xs:pattern>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>

  <xs:simpleType name="abs_filepath">
    <xs:annotation>
      <xs:appinfo>A well formed absolute file path.</xs:appinfo>
    </xs:annotation>
    <xs:union>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:length value="0"></xs:length>
        </xs:restriction>
      </xs:simpleType>
      <xs:simpleType>
        <xs:restriction base='xs:string'>
          <xs:pattern value="(([a-zA-Z]:(\\|/))|(/)|(\\\\)){1}([a-zA-Z0-9\s\._()-]+(\\|/)?)*\.([Xx][Mm][Ll]|[Zz][Ii][Pp]|[Pp][Ee][Mm]|[Pp][Pp][Kk]|[Hh][Tt][Mm][Ll]|[Hh][Tt][Mm]|[Tt][Xx][Tt]|[Dd][Bb])"></xs:pattern>
        </xs:restriction>
      </xs:simpleType>
    </xs:union>
  </xs:simpleType>

  <xs:simpleType name="rel_dirpath">
    <xs:annotation>
